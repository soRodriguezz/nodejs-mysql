<div class="container p-4">
    <div class="row">
        {{#each links}}
        <div class="col-md-3">
            <div class="card text-center">
                <div class="card body">
                    <a href="{{url}}">
                        <h3 class="card-title text-uppercase">
                            {{title}}
                        </h3>
                    </a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>